

#ifndef COTS_MCAL_UART_UART_REGSTER_H_
#define COTS_MCAL_UART_UART_REGSTER_H_
//REGESTER UCSRC AND UBRRH
#define URSEL  7
#define UMSEL  6 
#define UPM1   5
#define UPM0   4
#define USBS   3
#define UCSZ1  2
#define UCSZ0  1
#define UCPOL  0

//REGESTER UCSRA 
#define RXC   7
#define TXC   6 
#define UDRE  5
#define FE    4
#define DOR   3
#define PE    2
#define U2X   1
#define MPCM  0
//REGESTER UCSRB 
#define SPIF  7
#define WCOL  6

#define SPI2X   0


#define SPDR *((volatile unsigned char *)0x2F)
#define SPSR *((volatile unsigned char *)0x2E)
#define SPCR *((volatile unsigned char *)0x2D)


#endif /* COTS_MCAL_UART_UART_REGSTER_H_ */
